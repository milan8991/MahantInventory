@{
    ViewData["Title"] = "Parties";
}

<div class="card border-dark">
    <div class="card-header">
        <span class="font-weight-bold">Parties</span>
        <span class="float-right">
            <button class="btn btn-outline-primary btn-sm" type="button" onclick="Common.OpenModal(this)" data-id="0" data-target="AddEditParty">
                <span>New</span>
                </button>
        </span>
    </div>
    <div class="card-body ag-theme-balham" id="partiesdata">
    </div>
</div>
@await Html.PartialAsync("_AddEditParty",new MahantInv.Core.SimpleAggregates.Party())
@section Scripts{
<script src="~/Parties/index.js" type="text/javascript" asp-append-version="true"></script>
}
